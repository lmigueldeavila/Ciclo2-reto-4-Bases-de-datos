SELECT ser_titulo, can_resenia, can_anio FROM servicio, cancion WHERE can_anio > 1999;



SELECT * FROM cantante, cancion, servicio;
SELECT ser_titulo FROM servicio WHERE cant_nombre=Joe;


SELECT ser_titulo FROM servicio WHERE ser_id = (SELECT can_id FROM cancion WHERE can_cantante = (SELECT cant_id FROM cantante WHERE cant_nombre LIKE "Joe"));
SELECT ser_titulo FROM servicio,cancion, cantante WHERE servicio.ser_id = cancion.can_id and cantante.cant_nombre LIKE "Joe";

SELECT ser_titulo FROM servicio WHERE ser_id = (SELECT tra_ser FROM transmision WHERE tra_user LIKE "deadofwrite");
SELECT ser_titulo FROM servicio, transmision WHERE transmision.tra_user LIKE "deadofwrite";
